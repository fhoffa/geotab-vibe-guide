# Geotab API Credentials
# Copy this file to .env and fill in your actual credentials
# NEVER commit the .env file to git!

# Your Geotab database name (case-sensitive)
GEOTAB_DATABASE=your_database_name

# Your Geotab username (email)
GEOTAB_USERNAME=your_email@example.com

# Your Geotab password
GEOTAB_PASSWORD=your_password

# Geotab server (usually my.geotab.com for demo accounts)
GEOTAB_SERVER=my.geotab.com
